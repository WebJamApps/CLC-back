{
  "name": "breeze-chms",
  "version": "0.2.6",
  "description": "Provides convenient access to the Breeze REST API - https://app.breezechms.com/api.",
  "keywords": [
    "breeze",
    "chms",
    "Koa",
    "Node"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test-watch": "jest --config jestconfig.json --watch",
    "test:jest": "jest --config jestconfig.json --runInBand",
    "test:jest-u": "npm run test:jest -- -u",
    "watch": "tsc --watch",
    "clean": "rm -rf build",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "start:debug": "rm -rf build && tsc && npm-run-all -p ts-start test-watch",
    "prepublishOnly": "npm run test",
    "start": "node app.ts",
    "dev": "nodemon app.ts"
  },
  "main": "build/breeze.js",
  "types": "build/breeze.d.ts",
  "files": [
    "build/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WebJamApps/CLC-back.git"
  },
  "author": "Chris Doe <cdoe@notebird.app> (notebird.app)",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=9.5.1 <10"
  },
  "engineStrict": true,
  "dependencies": {
    "axios": "^1.4.0",
    "breeze-chms": "^0.2.6",
    "koa": "^2.14.2"
  },
  "devDependencies": {
    "@tsconfig/node14": "^14.1.0",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.3",
    "@types/koa": "^2.13.8",
    "dotenv": "^16.3.1",
    "jest": "^29.6.2",
    "jest-to-match-shape-of": "^1.3.2",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
